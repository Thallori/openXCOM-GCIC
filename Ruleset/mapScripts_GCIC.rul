mapScripts:
  - type: DEFAULT
    commands:
    - type: addUFO
    - type: addCraft
    - type: addBlock
      size: [2, 1]
    - type: addBlock
      size: [1, 2]
    - type: addBlock
      size: 2
      executions: 2
    - type: fillArea

  #makes sure the ufo isn't on the bottom edges of the map, since that's where doors will be and having to sneak down a 1-tile corridor that only exists because of the map's limits feels bad
  - type: DEFAULT4X4
    commands:
    - type: addUFO
      rects:
        - [0, 0, 3, 3]
    - type: addCraft
    - type: addBlock
      size: [2, 1]
      label: 1
      executionChances: 50
    - type: addBlock
      size: [1, 2]
      conditionals: [-1]
    - type: addBlock
      size: 2
      executions: 1
    - type: fillArea
  - type: DEFAULT5X5
    commands:
    - type: addUFO
      rects:
        - [0, 0, 4, 4]
    - type: addCraft
    - type: addBlock
      size: [2, 1]
    - type: addBlock
      size: [1, 2]
    - type: addBlock
      size: 2
    - type: fillArea

  - type: SINGLE_BLOCK_2X2
    commands:
    - type: addBlock
      size: 2

  - type: STUDIO_UFO_FOREST
    commands:
    - type: addUFO
      terrain: FOREST2
      rects:
        - [1, 1, 3, 3]
    - type: addBlock
      blocks: [0]
      rects:
        - [0, 0, 1, 6]
    - type: addBlock
      blocks: [1]
      rects:
        - [1, 0, 5, 1]
    - type: addBlock
      blocks: [2]
      rects:
        - [1, 5, 5, 1]
    - type: addBlock
      blocks: [3]
      rects:
        - [5, 1, 1, 4]
    - type: fillArea
      terrain: FOREST2


  - type: HANGAR_DEFAULT
    commands:
    - type: addUFO
    - type: addCraft
    - type: addLine
      direction: vertical
    - type: addBlock
      size: [1, 2]
    - type: addBlock
      size: 2
    - type: fillArea
  - type: HANGAR_DRONES_3X3
    commands:
    - type: addUFO
      rects:
        - [0, 0, 2, 2]
    - type: addCraft
    - type: addLine
      direction: vertical
#drone spawns
    - type: addBlock
      terrain: HANGAR_DRONES_1
      size: [1, 2]
      label: 1
      executionChances: 33
    - type: addBlock
      terrain: HANGAR_DRONES_1
      size: 2
      label: 2
      executionChances: 50
      conditionals: [-1]
    - type: addBlock
      terrain: HANGAR_DRONES_1
      conditionals: [-1, -2]
#rest of the owl
    - type: addBlock
      size: [1, 2]
      conditionals: [-1]
    - type: addBlock
      size: 2
      conditionals: [-2]
    - type: fillArea
